  "dependencies": {
    "child_process": "^1.0.2",
    "dotenv": "^17.2.1",
    "electron": "^37.2.6",
    "electron-store": "^10.1.0",
    "escpos": "^3.0.0-alpha.6",
    "escpos-network": "^3.0.0-alpha.5",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@electron/packager": "^18.4.0"
  }
}
