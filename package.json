{
  "name": "retro-cafe-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "set NODE_OPTIONS=--enable-source-maps --no-warnings && set NODE_ICU_DATA=node_modules/full-icu && electron .",
    "build": "npx @electron/packager . printerApp --platform=win32 --arch=x64 --out=dist --overwrite",
